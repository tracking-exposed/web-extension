{
  "name": "PAADC",
  "version": "1.1.4",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.11",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-replace": "^2.3.4",
    "@rollup/plugin-strip": "^1.3.3",
    "babel-jest": "^26.6.3",
    "chokidar": "^3.4.3",
    "dotenv": "^8.2.0",
    "jest": "^24.9.0",
    "jest-webextension-mock": "^3.7.4",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rollup": "^1.32.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "~6.1.1",
    "rollup-plugin-terser": "^5.3.1",
    "web-ext": "^5.4.1",
    "webextension-polyfill": "^0.6.0"
  },
  "scripts": {
    "_build": "rollup -c",
    "_build:watch": "rollup -c -w",
    "start": "run-s build dev",
    "clean": "rimraf build",
    "build": "run-s clean _build",
    "build:watch": "run-s clean _build:watch",
    "dist": "run-s build lint pack",
    "lint": "web-ext lint",
    "pack": "web-ext build",
    "test": "jest",
    "test:watch": "jest --watch",
    "dev": "run-p start:chromium build:watch",
    "dev:firefox": "run-p start:firefox build:watch",
    "start:firefox": "web-ext run --target=firefox-desktop --firefox-binary=$(which firefox) --start-url=www.facebook.com",
    "start:chromium": "web-ext run --target=chromium --chromium-binary=$(which chromium-browser) --start-url=www.facebook.com"
  },
  "dependencies": {
    "bootstrap": "^4.5.3",
    "bs58": "^4.0.1",
    "svelte": "^3.31.0",
    "svelte-spa-router": "^2.2.0",
    "sveltestrap": "^3.9.6",
    "tweetnacl": "^1.0.3",
    "uuid": "^3.4.0"
  },
  "webExt": {
    "sourceDir": "build",
    "artifactsDir": "dist",
    "build": {
      "overwriteDest": true
    }
  },
  "jest": {
    "setupFiles": [
      "jest-webextension-mock"
    ]
  }
}
