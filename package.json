{
  "name": "svelte-web-extension-template",
  "version": "0.0.1",
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/preset-env": "^7.7.5",
    "@rollup/plugin-alias": "^2.2.0",
    "@rollup/plugin-replace": "^2.2.1",
    "babel-jest": "^24.9.0",
    "chokidar": "^3.2.1",
    "dotenv": "^8.2.0",
    "jest": "^24.9.0",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.12.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-copy": "^3.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^4.0.4",
    "web-ext": "^3.2.0"
  },
  "scripts": {
    "start": "run-s build dev",
    "build": "rollup -c",
    "test": "jest",
    "test:watch": "jest --watch",
    "autobuild": "rollup -c -w",
    "dev": "run-p start:chromium autobuild",
    "dev:firefox": "run-p start:firefox autobuild",
    "start:firefox": "web-ext run --source-dir ./build/ --target=firefox-desktop --firefox-binary=$(which firefox) --start-url=www.facebook.com",
    "start:chromium": "web-ext run --source-dir ./build/ --target=chromium --chromium-binary=$(which chromium-browser) --start-url=www.facebook.com"
  },
  "dependencies": {
    "bs58": "^4.0.1",
    "svelte": "^3.0.0",
    "sveltestrap": "^3.2.8",
    "tweetnacl": "^1.0.1",
    "uuid": "^3.3.3",
    "webextension-polyfill": "^0.5.0"
  }
}
